{
  "expo": {
    "name": "vivesaude-labios-expo",
    "slug": "vivesaude-labios-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.vcsaude.vivesaudelabios",
      "supportsTablet": true
    },
    "android": {
      "package": "com.vcsaude.vivesaudelabios",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "locales": {
      "pt": "./assets/strings/pt.json"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 25
          }
        }
      ],
      "@logrocket/react-native",
      "expo-font",
      "expo-secure-store"
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d7a051e2-c02d-4c2b-b889-54e7096f4323"
    },
    "extra": {
      "eas": {
        "projectId": "d7a051e2-c02d-4c2b-b889-54e7096f4323"
      }
    },
    "owner": "vcsaude"
  }
}
